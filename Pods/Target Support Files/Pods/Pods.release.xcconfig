GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AsyncDisplayKit" "${PODS_ROOT}/Headers/Public/MJRefresh" "${PODS_ROOT}/Headers/Public/Shimmer" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AsyncDisplayKit" -isystem "${PODS_ROOT}/Headers/Public/MJRefresh" -isystem "${PODS_ROOT}/Headers/Public/Shimmer" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AsyncDisplayKit" -l"MJRefresh" -l"Shimmer" -l"c++" -l"pop" -framework "AssetsLibrary" -weak_framework "Photos"
PODS_ROOT = ${SRCROOT}/Pods